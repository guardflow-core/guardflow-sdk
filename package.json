  },
  "bugs": {
    "url": "https://github.com/guardflow/guardflow-sdk/issues"
  },
  "homepage": "https://github.com/guardflow/guardflow-sdk#readme",
  "dependencies": {
    "web3": "^4.3.0",
    "ethers": "^6.8.1",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "big.js": "^6.2.1"
